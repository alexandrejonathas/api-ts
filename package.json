{
  "name": "ts-api",
  "version": "1.0.0",
  "description": "Api criada no curso de TypeScript na pratica",
  "main": "./server/server.ts",
  "scripts": {
    "start": "PORT=5000 NODE_ENV=development ./node_modules/.bin/ts-node ./server/server.ts",
    "builded": "PORT=5000 NODE_ENV=production node ./dist/server/server.js",
    "unit-test": "NODE_ENV=test mocha --opts mocha.opts ./dist/tests/unit/unit.test.js",
    "integration-test": "NODE_ENV=test mocha --opts mocha.opts ./dist/tests/integration/integration.test.js",
    "integration-coverage": "nyc --reporter=html npm run integration-test",
    "unit-coverage": "nyc --reporter=html npm run unit-test"
  },
  "author": "jlima",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^3.0.3",
    "bluebird": "^3.5.3",
    "body-parser": "^1.18.3",
    "express": "^4.16.4",
    "http": "0.0.0",
    "http-status": "^1.3.1",
    "jwt-simple": "^0.5.5",
    "lodash": "^4.17.11",
    "morgan": "^1.9.1",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "pg": "^7.7.1",
    "sequelize": "^4.42.0",
    "sequelize-cli": "^5.4.0",
    "ts-node": "^7.0.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/bluebird": "^3.5.25",
    "@types/body-parser": "^1.17.0",
    "@types/chai": "^4.1.7",
    "@types/express": "^4.16.0",
    "@types/http-status": "^0.2.30",
    "@types/jwt-simple": "^0.5.33",
    "@types/lodash": "^4.14.119",
    "@types/mocha": "^2.2.41",
    "@types/morgan": "^1.7.35",
    "@types/passport": "^1.0.0",
    "@types/passport-jwt": "^3.0.1",
    "@types/sequelize": "^4.27.33",
    "@types/supertest": "^2.0.7",
    "chai": "^4.2.0",
    "gulp": "^3.9.1",
    "gulp-clean": "^0.4.0",
    "gulp-typescript": "^5.0.0",
    "istanbul": "^0.4.5",
    "mocha": "^3.2.0",
    "nyc": "^13.1.0",
    "supertest": "^3.3.0",
    "testdouble": "^3.9.2",
    "ts-node": "^7.0.1",
    "typescript": "^3.2.2"
  }
