{
  "name": "ts-api",
  "version": "1.0.0",
  "description": "Api criada no curso de TypeScript na pratica",
  "main": "server/server.ts",
  "scripts": {
    "start": "set \"NODE_ENV=development\" && ./node_modules/.bin/ts-node ./server/server.ts",
    "builded": "set \"NODE_ENV=production\" && node ./build/server/server.js",
    "unit-test": "set \"NODE_ENV=test\" && mocha --opts ./build/tests/unit/config/mocha.opts ./build/tests/unit/unit.test.js",
    "integration-test": "set \"NODE_ENV=test\" && mocha --opts ./build/tests/integration/config/mocha.opts ./build/tests/integration/integration.test.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "jlima",
  "license": "ISC",
  "dependencies": {
    "bluebird": "^3.5.3",
    "body-parser": "^1.18.3",
    "express": "^4.16.4",
    "http": "0.0.0",
    "http-status": "^1.3.1",
    "jwt-simple": "^0.5.5",
    "lodash": "^4.17.11",
    "morgan": "^1.9.1",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "pg": "^7.7.1",
    "sequelize": "^4.42.0",
    "sequelize-cli": "^5.4.0",
    "ts-node": "^7.0.1"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.25",
    "@types/body-parser": "^1.17.0",
    "@types/chai": "^4.1.7",
    "@types/express": "^4.16.0",
    "@types/http-status": "^0.2.30",
    "@types/jwt-simple": "^0.5.33",
    "@types/lodash": "^4.14.119",
    "@types/mocha": "^5.2.5",
    "@types/morgan": "^1.7.35",
    "@types/passport": "^1.0.0",
    "@types/passport-jwt": "^3.0.1",
    "@types/sequelize": "^4.27.33",
    "@types/supertest": "^2.0.7",
    "chai": "^4.2.0",
    "mocha": "^5.2.0",
    "supertest": "^3.3.0",
    "testdouble": "^3.9.2",
    "ts-node": "^7.0.1",
    "typescript": "^3.2.2"
  }
}
